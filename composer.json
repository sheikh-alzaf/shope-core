{
    "name": "shope/core",
    "description": "Shared common package for Shope microservices",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shope\\Core\\": "src/"
        },
        "files": [
            "src/Supports/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
